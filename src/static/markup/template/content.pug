include ../components/_base/index

<!DOCTYPE html>
html(lang="ru-RU")
    head
        block page-title
        include ../components/_base/meta-head
    body
        +b.page
            +e.header
                include ../components/header

            +e.main
                +b.content
                    +e.wrapper.wrapper
                        +e.left
                            block content-left
                        +e.right
                            block content-right

            +e.footer
                include ../components/footer
        +b.humburger.js-mobile-panel-open
            +e.item
            +e.item
            +e.item
        include ../components/mobile-panel

        include ../components/_base/scripts